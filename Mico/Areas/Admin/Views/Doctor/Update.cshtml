@model Doctor
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Title">Enter Title</label>
        <input asp-for="Title" type="text" class="form-control" placeholder="Enter email">
        <span asp-validation-for="Title"></span>
    </div>

    <div class="form-group">
        <label asp-for="Name">Enter Name</label>
        <input asp-for="Name" type="text" class="form-control" placeholder="Enter Name">
        <span asp-validation-for="Title"></span>
    </div>
    <div class="form-group">
        <label asp-for="Duty">Enter Duty</label>
        <input asp-for="Duty" type="text" class="form-control" placeholder="Enter Duty">
        <span asp-validation-for="Duty"></span>
    </div>
    <div class="form-group">
        <label asp-for="PhotoUrl">Enter Photo</label>
        <input name="file" type="file" class="form-control" placeholder="Enter Photo">
        <span asp-validation-for="Title"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
